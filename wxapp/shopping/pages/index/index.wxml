<view class="main">
    <swiper indicator-dots="{{true}}" autoplay="true" circular="true" duration="{{duration}}" vertical="{{vertical}}">
        <block wx:for="{{imgUrls}}" wx:key="index">
            <swiper-item class="" item-id="">
                <image class="slide-image" src="{{item}}" width="100%" />
            </swiper-item>
        </block>
        </swiper>
     <view class="select">
